{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "supportedNodeIdes": {
        "node": "20.x"
    },
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.ts"
        }
    ]
}